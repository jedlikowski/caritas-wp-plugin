{
  "name": "indicoplus/caritas-wp-plugin",
  "type": "project",
  "version": "1.3.1",
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "dist"
  },
  "require": {
    "php": ">=7.1",
    "yahnis-elsts/plugin-update-checker": "^4.9",
    "jedlikowski/wordpress-router": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/jedlikowski/WP_Router"
    }
  ],
  "autoload": {
    "psr-4": {
      "IndicoPlus\\CaritasApp\\": "lib/"
    }
  },
  "scripts": {
    "prepare-release": "zip -r -X caritas-wp-plugin.zip . -x \"*.git*\" -x \"*.DS_Store\""
  }
}
